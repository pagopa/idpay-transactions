{
  "request": {
    "method": "GET",
    "urlPathPattern": "/tokens/(USERID_OK_[0-9]+)/pii",
    "headers": {
      "x-api-key": {
        "matches": "x_api_key([0-9]?)+"
      }
    }
  },
  "response": {
    "status": "200",
    "jsonBody": {
      "pii": "fiscalCode"
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}